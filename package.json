{
  "name": "any2api-gateway",
  "version": "0.0.1",
  "description": "any2api gateway",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "ts-node lib/index.ts",
    "prepublishOnly": "npm run build",
    "build": "tsc -p ./lib/tsconfig.lib.json --outDir dist/",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/any2api/any2api-gateway.git"
  },
  "keywords": [
    "any2api",
    "gateway",
    "grpc"
  ],
  "author": "Johannes Wettinger",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/any2api/any2api-gateway/issues"
  },
  "homepage": "https://github.com/any2api/any2api-gateway#readme",

  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.86",
    "@types/node": "^8.0.53",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.3",
    "ts-lint": "^4.5.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },

  "dependencies": {
    "@reactivex/rxjs": "^5.5.6",
    "google-proto-files": "^0.14.1",
    "grpc": "^1.7.2",
    "lodash": "^4.17.4",
    "protobufjs": "~6.8.3"
  },

  "jest": {
    "transform": {
      "^.+\\.ts?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js?|ts?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
