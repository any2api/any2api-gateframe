{
    "extends": "tslint:recommended",
    "rules": {
        "align": [
            true,
            "parameters",
            "arguments",
            "statements"
        ],
        "eofline": true,
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-line-length": {
            "options": [
                120
            ]
        },
        "max-classes-per-file": false,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": true,
        "no-console": {
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ],
            "severity": "warning"
        },
        "no-construct": true,
        "no-debugger": true,
        "no-duplicate-variable": true,
        "no-empty": true,
        "no-eval": true,
        "no-shadowed-variable": true,
        "no-string-literal": true,
        "no-switch-case-fall-through": true,
        "no-trailing-whitespace": false,
        "no-unused-expression": true,
        "no-use-before-declare": true,
        "quotemark": [
            true,
            "single"
        ],
        "semicolon": [
            true,
            "always"
        ],
        "trailing-comma": [
            false
        ],
        "triple-equals": [
            true,
            "allow-null-check"
        ],
        "ordered-imports": false,
        "member-ordering": false
    }
}
